{
  "name": "eloquent-form-builder",
  "version": "1.0.0",
  "description": "A transparent, es6 class based form builder designed to work out of the box with Laravel's Eloquent Forms framework. A dev can easily go into the individual classes and add or customize functionality to best fit their backend functionality",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run watch & npm run dev",
    "test": "karma start karma.conf.js",
    "dev": "webpack-dev-server --mode development --open",
    "watch": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/booellean/eloquent-form-builder.git"
  },
  "keywords": [
    "form",
    "builder",
    "form",
    "builder",
    "drag",
    "and",
    "drop",
    "transparent"
  ],
  "author": "Elle (booellean) Pope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/booellean/eloquent-form-builder/issues"
  },
  "homepage": "https://github.com/booellean/eloquent-form-builder#readme",
  "devDependencies": {
    "@types/node": "^12.12.16",
    "autoprefixer": "^9.7.3",
    "bulma": "^0.8.0",
    "css-loader": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^4.1.1",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {}
}
